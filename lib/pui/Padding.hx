package pui;

/**
 * Отступы для позицианирования контента.
 */
typedef Padding =
{
    /**
     * Верхний отступ. (px)
     */
    var top:Float;

    /**
     * Левый отступ. (px)
     */
    var left:Float;
    
    /**
     * Правый отступ. (px)
     */
    var right:Float;
    
    /**
     * Нижний отступ. (px)
     */
    var bottom:Float;
}