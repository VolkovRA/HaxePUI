package pui;

/**
 * Отступы.
 * Используется для задания внешних (margin) и внутренних (padding) отступов.
 */
typedef Offset =
{
    /**
     * Верхний отступ. (px)
     */
    var top:Float;

    /**
     * Левый отступ. (px)
     */
    var left:Float;

    /**
     * Правый отступ. (px)
     */
    var right:Float;

    /**
     * Нижний отступ. (px)
     */
    var bottom:Float;
}